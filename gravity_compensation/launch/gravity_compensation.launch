<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="end_effector" default="pal-hey5"/>
  <arg name="robot" default="tiago"/>
  <arg name="simulation" default="true"/>
  <arg name="spawn" default="true"/>
  <arg name="controller_ns" default="gravity_compensation"/>


  <rosparam command="load" file="$(find gravity_compensation)/config/tiago_pal-hey5_gravity_compensation_simulation.yaml" ns="$(arg controller_ns)"/>
  <node name="$(arg controller_ns)" pkg="controller_manager" type="spawner" output="screen" args="--timeout 120 $(arg controller_ns)" />
  

</launch>
